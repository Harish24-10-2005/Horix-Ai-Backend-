# Environment Configuration for MCP Auto Configuration Agent
# Copy this file to .env and fill in your API keys

# Groq API Key for LangChain integration
GROQ_API_KEY=your_groq_api_key_here

# Optional: OpenAI API Key (fallback)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: GitHub Personal Access Token (for better rate limits)
GITHUB_TOKEN=your_github_token_here

# MCP Configuration Settings
MCP_AUTO_CONFIG_ENABLED=true
MCP_CONFIG_BACKUP_ENABLED=true
MCP_VERBOSE_LOGGING=true
